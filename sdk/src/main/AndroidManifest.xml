<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.swedbankpay.mobilesdk">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application>
        <activity android:name=".internal.ToSActivity" android:label="@string/swedbankpaysdk_tos"/>
        <activity android:name=".internal.CallbackActivity">
            <intent-filter android:priority="-100">
                <!--  Priority is negative on purpose. This way you can
                 easily override this intent filter in your app. -->
                <action android:name="com.swedbankpay.mobilesdk.VIEW_PAYMENTORDER" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:scheme="https" />
                <data android:scheme="http" />
            </intent-filter>
        </activity>
    </application>
</manifest>
